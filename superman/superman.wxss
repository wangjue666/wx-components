}
@keyframes superman_fadeout{
    0% {
       opacity:1;
       transform:scale(0.32);
    }
    100%{
        opacity:0;
        transform:scale(0.1);
    }
}
@keyframes superman_shadow {
    50% { transform: translateY(20px) scale(1); opacity:.05; }
}

