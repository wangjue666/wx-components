.superman:after {
    animation: 3s ease-in-out 0s normal none infinite superman_shadow;
    background: none repeat scroll 0 0 #000000;
    border-radius: 50%;
    bottom: -201px;
    content: "";
    height: 20px;
    left: -24px;
    opacity: 0.2;
    position: absolute;
    width: 165px;

}
@keyframes superman_fly{
    0% {
        transform:translate(0rpx,0rpx);
    }
    20%{
        transform:rotateY(0) translate(-700rpx,-800rpx);
    }
    70%{
        transform: rotateY(180deg)translate(500rpx,-800rpx);
    }
    90%{
        transform: rotateY(180deg)translate(-3000rpx,-800rpx);
    }

